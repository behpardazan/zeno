@model DataLayer.Entities.ProductCustomField
@{
    ViewBag.Title = @Resource.Lang.RemoveInfo;
    Layout = "~/Views/Shared/Layout.cshtml";
}
<div class="col-sm-12">
    <div class="card-box">
        <a href="@Url.Action("index", new {
                    typeId = Model.ProductTypeId,
                    categoryId = Model.ProductCategoryId,
                    subcategoryId = Model.ProductSubCategoryId,
                    brandId = Model.ProductBrandId
                })" class="icon circle-icon glyphicon glyphicon-arrow-left" data-toggle="tooltip" data-placement="bottom" title=   @Resource.Lang.Back></a>
        <div class="col-sm-12">
            @if (ViewBag.Alert != null)
            {
                <p class="alert alert-danger">@ViewBag.Alert</p>

            }
            <div class="form-horizontal">
                <h3>@Resource.Lang.SureDelete</h3>
                <dl class="dl-horizontal">
                    <dt>
                        @Resource.Lang.TypeProduct
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.ProductType.Name)
                    </dd>
                    <dt>
                        @Resource.Lang.FieldName
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Name)
                    </dd>
                    <dt>
                        @Resource.Lang.TypeField
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Code.Name)
                    </dd>
                    <dt>
                        @Resource.Lang.PriorityShow
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.ShowNumber)
                    </dd>
                    <dt>
                        @Resource.Lang.RemoteServer
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.SyncName)
                    </dd>
                    <dt>
                        @Resource.Lang.Forced_NotForced
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.IsRequired)
                    </dd>
                </dl>
            </div>
            <div class="form-group">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    <input type="hidden" value="@Model.ID" id="ID" name="ID" />

                    <div class="form-actions no-color">
                        <input type="submit" value=@Resource.Lang.Delete class="btn btn-danger" />
                        <span class="btn btn-inverse">
                            @Html.ActionLink(Resource.Lang.Back, "index", new
                       {
                           typeId = Model.ProductTypeId,
                           categoryId = Model.ProductCategoryId,
                           subcategoryId = Model.ProductSubCategoryId,
                           brandId = Model.ProductBrandId
                       })
                        </span>

                    </div>
                }
            </div>
        </div>
        <div class="clearfix"></div>
    </div>
</div>