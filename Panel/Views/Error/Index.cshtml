@model DataLayer.ViewModels.ViewError

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/ErrorLayout.cshtml";
}

<div class="text-error">@Model.Code</div>
<h3 class="text-uppercase font-600">@Model.Title</h3>
<p class="text-muted">
    @Model.Message
</p>
<br>

<a class="btn btn-success waves-effect waves-light" href="@Url.Action("logout", "profile")">@Resource.Lang.Logout</a>
<a class="btn btn-success waves-effect waves-light" href="@Model.BackUrl">@Resource.Lang.Back</a>


