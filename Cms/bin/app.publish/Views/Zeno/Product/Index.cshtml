@model DataLayer.Entities.Product
@using DataLayer.Base;
@using DataLayer.Enumarables;
@{

    Layout = Resources.Zeno.WEBSITE_LAYOUTIndex;
    Nullable<double> totalRate = 0;
    if (Model.ProductComment.Any())
    {
        totalRate = Model.ProductComment.Where(s => s.Rate.HasValue).Average(s => s.Rate);
    }
    ViewBag.Title = Model.Title != null ? Model.Title : Model.GetName();

    ViewBag.OgUrl = Model.GetLinkWithUrl();
    ViewBag.url = Model.GetLinkWithUrl();
    ViewBag.Canonical = Model.Canonical;
    var call = Model.ProductCustomValue.FirstOrDefault(s => s.ProductCustomField.SyncName == "Call" && s.Value == "True");

    ViewBag.OgImage = Model.Picture != null ? Model.Picture.GetUrl() : "/Assets/Zeno/Image/side1.jpg";
    ViewBag.OgImageType = "jpg";
    TempData["noIndex"] = Model.NoIndex;
    TempData["noFollow"] = Model.NoFollow;
    ViewBag.MetaDescription = Model.GetSummery();
    try
    {
        if (Model.Code.Label == Enum_Code.PRODUCT_STATUS_AVAILABLE.ToString())
        {
            ViewBag.MinPrice = Model.MinPrice;

            if (Model.ProductQuantity.Max(s => s.StoreProductQuantity.Max(g => g.Price)) > 0)
            {
                ViewBag.Price = Model.Price;
            }
            if (Model.ProductQuantity.Average(s => s.StoreProductQuantity.Average(g => g.Count)) > 0)
            {
                ViewBag.Stock = Model.ProductQuantity.Average(s => s.StoreProductQuantity.Average(g => g.Count));
            }
        }
        else
        {
            ViewBag.MinPrice = Resource.Lang.NoT_AVAILABLE;
            ViewBag.Price = Resource.Lang.NoT_AVAILABLE;
            ViewBag.Stock = 0;
        }
    }
    catch
    {

    }


    string url = "";
    totalRate = totalRate == 0 || totalRate == null ? 4 : totalRate;
    //url = HttpContext.Current.Request.Url.AbsoluteUri;
    var setting = BaseWebsite.WebsiteSetting;
    var ratingCount = Model.GetCountRate() == 0 || Model.GetCountRate() == null ? 5 : Model.GetCountRate();
    var DiscountPercent = Model.GetDiscountPercent();
    var DiscountValue = Model.GetDiscountObject();
    var TotalPrice = Model.MinPrice;
    var price = "";
    var oldePrice = "";
    if (DiscountValue != null)
    {
        if (DiscountValue.PriceValue != null)
        {
            TotalPrice += DiscountValue.PriceValue.Value;
        }
        if (DiscountPercent == 0)
        {
            DiscountPercent = Model.GetDiscountPercentNew(DiscountValue.PriceValue.GetValueOrDefault());

        }
        else
        {
            var percent = 100 - DiscountPercent;
            TotalPrice = (Model.MinPrice * 100) / percent;
        }

    }
    if (DiscountValue != null)
    {
        oldePrice = @TotalPrice.GetCurrencyFormat();
        price = @Model.MinPrice.GetCurrencyFormat();

    }
    else
    {
        oldePrice = price = @Model.MinPrice.GetCurrencyFormat();
    }
}

@section Header {
    <meta name="product_id" content='@Model.ID' />
    <meta name="page_url" content='@Model.GetLink()' />
    <meta name="price" content='@(Model.Code.Label == DataLayer.Enumarables.Enum_Code.PRODUCT_STATUS_AVAILABLE.ToString() ? price : Resource.Lang.NoAvailable)' />
    <meta name="old_price" content='@(Model.Code.Label == DataLayer.Enumarables.Enum_Code.PRODUCT_STATUS_AVAILABLE.ToString() ? oldePrice : Resource.Lang.NoAvailable)' />
    <meta name="availability" content='@(Model.Code.Label == DataLayer.Enumarables.Enum_Code.PRODUCT_STATUS_AVAILABLE.ToString() ? "instock" : Resource.Lang.NoAvailable)' />

    <script type="application/ld+json">
        {
        "@@context": "https://schema.org/",
        "@@type": "Product",
        "name": "@Model.Name",
        "image": "@Model.Picture.GetUrl()",
        "description": "@Model.Summary",
        "brand": {
        "@@type": "Brand",
        "name": "@(Model.ProductBrand != null ? Model.ProductBrand.Name : "")"
        },
        "review": {
        "@@type": "Review",
        "author": {
        "@@type": "Person",
        "name": "@BaseContent.WebsiteDetail.Title"
        },
        "reviewRating": {
        "@@type": "Rating",
        "ratingValue": "@totalRate",
        "bestRating": "5"
        }
        },
        "aggregateRating": {
        "@@type": "AggregateRating",
        "ratingValue": "@totalRate",
        "bestRating": "5",
        "worstRating": "1",
        "ratingCount": "@Model.GetCountRate()"
        }
        }

    </script>
    @Html.Raw(Model.Schema)
}
<nav aria-label="breadcrumb" class="BREADCRUMB">
    <div class="container">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">@Resource.Lang.Home</a></li>
            @try
            {
                if (Model.ProductTypeId != null)
                {
                    <li class="breadcrumb-item">

                        <a href="@Model.ProductType.GetUrlNew()">@Model.ProductType.GetName()</a>
                    </li>
                }
                if (Model.ProductCategoryId != null)
                {
                    if (Model.ProductCategory.Deleted != true)
                    {

                        <li class="breadcrumb-item">
                            <a href="@Model.ProductCategory.GetUrlNewZeno()">@Model.ProductCategory.GetName()</a>
                        </li>
                    }
                }
                if (Model.ProductSubCategoryId != null)
                {
                    if (Model.ProductSubCategory.Deleted != true)
                    {
                        <li class="breadcrumb-item">

                            <a href="@Model.ProductSubCategory.GetUrlNewZeno()">@Model.ProductSubCategory.GetName()</a>
                        </li>
                    }
                }
            }
            catch { }
        </ol>
    </div>
</nav>
