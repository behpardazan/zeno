        </div>
        <!--/ko-->

        <div class="proDetail_box">
            <div class="bordered_title">
                <strong>@Resource.Lang.AboutProduct</strong>
            </div>
            <div class="product_explainations">
                <ul class="nav nav-tabs" id="product_description_tabs" role="tablist">
                    @{ var ActiveClass = "show active";
                        var NavLinkActive = "active";}
                    @if (Model.GetDescription() != "" && Model.GetDescription() != null)
                    {
                        ActiveClass = "";
                        NavLinkActive = "";
                        <li class="nav-item">
                            <a class="nav-link active" id="description_tab" data-bs-toggle="tab" href="#description"
                               role="tab" aria-controls="description" aria-selected="true">@Resource.Lang.Description</a>
                        </li>
                    }
                    <li class="nav-item">
                        <a class="nav-link @NavLinkActive" id="specification_tab" data-bs-toggle="tab" href="#profileTab_content"
                           role="tab" aria-controls="profileTab_content" aria-selected="false">@Resource.Lang.Detail</a>
                    </li>
                    @{
                        if (Model.Review.Count > 0)
                        {
                            <li class="nav-item">
                                <a class="nav-link" id="reviw_tab" data-bs-toggle="tab" href="#reviewTab_content" role="tab" aria-controls="review_content"
                                   aria-selected="false">@Resource.Lang.Review</a>
                            </li>
                        }
                    }
                    @if (Model.ProductVideo.Count > 0)
                    {
                        <li class="nav-item">
                            <a class="nav-link" id="video_tab" data-bs-toggle="tab" href="#video_content" role="tab"
                               aria-controls="video_content" aria-selected="false">مشاهده ویدئو</a>
                        </li>
                    }
                    <li class="nav-item">
                        <a class="nav-link" id="comment_tab" data-bs-toggle="tab" href="#commenTab_content"
                           role="tab" aria-controls="commenTab_content" aria-selected="false">@Resource.Lang.Comments</a>
                    </li>
                </ul>
                <div class="tab-content mt-0" id="peoductDescription_Content">
                    @if (Model.GetDescription() != "" && Model.GetDescription() != null)
                    {
                        <div class="tab-pane fade show active" id="description" role="tabpanel"
                             aria-labelledby="description_tab">
                            <p>
                                @Html.Raw(Model.GetDescription())
                            </p>
                        </div>
                    }
                    <div class="tab-pane fade @ActiveClass" id="profileTab_content" role="tabpanel"
                         aria-labelledby="specification_tab">
                        <div class="specifications">
                            @foreach (var item in Model.ProductCustomValue)
                            {
                                if (item.ProductCustomField.SyncName != "Specialproduct" && item.ProductCustomField.SyncName != "SpecialProduct" && item.ProductCustomField.SyncName != "Call" && item.ProductCustomField.SyncName != "HomeProductHaveVideo")
                                {
                                    if ((item.Value != "" || item.ProductCustomItem.Value != ""))
                                    {
                                        <div class="specification_one">
                                            <div class="specification_title"><span>@item.ProductCustomField.Name:</span></div>
                                            @if (item.ItemId == null)
                                            {
                                                <div class="specification_text">@item.Value</div>
                                            }
                                            else
                                            {
                                                <div class="specification_text">@item.ProductCustomItem.Value</div>
                                            }
                                        </div>
                                    }
                                }

                            }

                        </div>
                    </div>
                    <div class="tab-pane fade" id="reviewTab_content" role="tabpanel" aria-labelledby="reviw_tab">
                        <div class="product_reviw">
                            <div class="row">
                                @foreach (var item in Model.Review)
                                {
                                    <div class="col-sm-6  mb-3">
                                        <div class="reviewInfoBox">
                                            <p>@item.Contect</p>
                                        </div>
                                    </div>
                                    <div class="col-sm-6 mb-3">
                                        <div class="reviewImgBox ">
                                            <img src="@item.Picture.GetUrl()" />
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade " id="video_content" role="tabpanel" aria-labelledby="video_tab">
                        @if (Model.ProductVideo.Count() > 0)
                        {
                            <div class="row">
                                @foreach (var item in Model.ProductVideo)
                                {
                                    <div class="col-lg-6">@Html.Raw(item.Url)</div>
                                }
                            </div>


                        }
                    </div>
                    <div class="tab-pane fade" id="commenTab_content" role="tabpanel" aria-labelledby="comment_tab">
                        <div class="product_comments">

                            @foreach (var item in Model.ProductComment.Where(p => p.Approved == true).OrderByDescending(p => p.ID).Take(10))
                            {
                                <div class="comment_item">
                                    <div class="comment_info">
                                        <div class="comment_userImg online_user"><img src="/assets/zeno/images/user.png" alt="@item.Account.FullName"></div>
                                        <div class="comment_name_date">
                                            <div href="#">
                                                <strong>@item.Account.FullName</strong>
                                                @Html.Partial("~/Views/" + BaseWebsite.WebsiteLabel + "/Partial/ShowRate.cshtml", item.Rate, new ViewDataDictionary())
                                            </div>
                                            <span>@item.Datetime.ToPersianWithTime()</span>
                                        </div>
                                    </div>
                                    <div class="comment_text">
                                        <p>@item.Body</p>
                                    </div>
                                    <div class="comment_reply">
                                        @if (!string.IsNullOrEmpty(item.AnswerString))
                                        {
                                            <div class="reply_item">
                                                <div class="comment_info">
                                                    <div class="comment_userImg offline_user"><img src="~/Assets/KhoshKala/Image/download.png" alt=""></div>
                                                    <div class="comment_name_date">
                                                        <a href="#">
                                                            <strong>@Resource.Lang.Boss</strong>
                                                        </a><span>@item.AnswerDatetime.ToPersianWithTime()</span>
                                                    </div>
                                                </div>
                                                <div class="comment_text">
                                                    @item.AnswerString
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            }
                            <div class="comment_item">
                                <div class="add_comment">

                                    <strong>@Resource.Lang.AddComment</strong>
                                    <form class="addCm_form text-left">
                                        <div class="star-rating user-rate">
                                            <input id="star-5" type="radio" name="rate" value="5" />
                                            <label for="star-5" title="5 stars">
                                                <i class="active bi bi-star-fill" aria-hidden="true"></i>
                                            </label>
                                            <input id="star-4" type="radio" name="rate" value="4" />
                                            <label for="star-4" title="4 stars">
                                                <i class="active bi bi-star-fill" aria-hidden="true"></i>
                                            </label>
                                            <input id="star-3" type="radio" name="rate" value="3" />
                                            <label for="star-3" title="3 stars">
                                                <i class="active bi bi-star-fill" aria-hidden="true"></i>
                                            </label>
                                            <input id="star-2" type="radio" name="rate" value="2" />
                                            <label for="star-2" title="2 stars">
                                                <i class="active bi bi-star-fill" aria-hidden="true"></i>
                                            </label>
                                            <input id="star-1" type="radio" name="rate" value="1" />
                                            <label for="star-1" title="1 star">
                                                <i class="active bi bi-star-fill" aria-hidden="true"></i>
                                            </label>
                                        </div>
                                        <textarea codeprocess-field="body" name="comment" class="form-control" id="" placeholder="@Resource.Lang.YourComment"></textarea>
                                        <button type="button" codeprocess-action="comment-add" product-id="@Model.ID" class="addCm_btn btn primary_btn  mt-4">
                                            <span>
                                                @Resource.Lang.Send
                                            </span>
                                        </button>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="modal fade" id="sqr-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered d-flex justify-content-center align-items-center">
        <div class="qr-modal-body text-center">
            <img src="/product/getqr?productId=@(Model.ID)" alt="" />
            <button class="btn btn-sm btn-danger closeQr" onclick="qrModalClose()">x</button>
        </div>
    </div>
</div>
<input type="hidden" class="addcount form-control" placeholder="1" value="1" />

@section endScripts {
    <script type="text/javascript">
        initializeProductPageScript();
        function plusCount() {
            var count = parseInt($(".addcount").val());
            var count = count + 1;
            $(".countP").text(count);
            $(".addcount").val(count);
        }
        function minusCount() {
            var count = parseInt($(".addcount").val());
            count = count - 1;
            if (count == 0) {
                count = 1;
                $(".countP").text(count);
                $(".addcount").val(count);
            }
            else {
                $(".countP").text(count);
                $(".addcount").val(count);
            }
        }
    </script>


}