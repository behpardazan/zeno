
@using DataLayer.Base;
@model IEnumerable<DataLayer.Entities.Post>



<div class="innerPage_box questionBox">
    @if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {
            <div class="question_item">
                <a href="@item.GetLinkWithUrl()">@item.GetSummery()</a>
                <a href="@item.GetLinkWithUrl()" class="btn morebtn expand_question">@Resource.Lang.SeeMore</a>
            </div>
        }
    }

</div>
